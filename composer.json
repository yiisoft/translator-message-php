{
    "name": "yiisoft/i18n-message-php",
    "type": "library",
    "description": "Yii i18n PHP message storage",
    "keywords": [
        "i18n",
        "internationalization",
        "formatting",
        "message storage"
    ],
    "homepage": "http://www.yiiframework.com/",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/i18n/issues?state=open",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/i18n"
    },
    "require": {
        "php": "^7.4",
        "yiisoft/i18n": "3.0.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Yiisoft\\I18n\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yiisoft\\I18n\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
