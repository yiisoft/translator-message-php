{
    "name": "yiisoft/translator-message-php",
    "type": "library",
    "description": "Yii PHP message storage",
    "keywords": [
        "i18n",
        "internationalization",
        "formatting",
        "message storage"
    ],
    "homepage": "http://www.yiiframework.com/",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/translator-message-php/issues?state=open",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/translator-message-php"
    },
    "require": {
        "php": "^7.4|^8.0",
        "yiisoft/translator": "3.0.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3",
        "phan/phan": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Yiisoft\\Translator\\Message\\Php\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yiisoft\\Translator\\Message\\Php\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
